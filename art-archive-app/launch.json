{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome", // or "edge", if you want to use Edge
        "request": "launch",
        "name": "Launch Chrome against localhost",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src", // adjust this to your source files
        "sourceMapPathOverrides": {
          "webpack:///src/*": "${webRoot}/*"
        }
      }
    ]
  }
  